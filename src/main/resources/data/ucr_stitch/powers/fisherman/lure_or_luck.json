{
    "name": "Lure or Luck",
    "description": "You have +2 Lure and -1 Luck of the Sea. Toggle this ability to change it to +2 Luck of the Sea and -1 Lure instead.",
    "type": "origins:multiple",
    "toggle": {
        "type": "origins:toggle",
        "active_by_default": false,
        "key": {
            "key": "key.origins.primary_active"
        },
        "retain_state": true
    },
    "ability": {
        "type": "origins:active_self",
        "hidden": true,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:power_active",
                        "power": "ucr_stitch:fisherman/lure_or_luck_toggle"
                    },
                    "if_action": {
                        "type": "origins:execute_command",
                        "command": "tellraw @p {\"text\":\"You now have +2 Luck of the Sea and -1 Lure!\",\"color\":\"blue\"}"
                    },
                    "else_action": {
                        "type": "origins:execute_command",
                        "command": "tellraw @p {\"text\":\"You now have +2 Lure and -1 Luck of the Sea!\",\"color\":\"blue\"}"
                    }
                }
            ]
        },
        "cooldown": 1,
        "hud_render": {
            "should_render": false
        },
        "key": {
            "key": "key.origins.primary_active"
        }
    },
