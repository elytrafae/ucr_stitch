{
    "name": "Propel",
    "description": "Propel yourself in the direction you are looking horizontally, and upwards vertically. If you are stationary horizontally, you are only launched upwards. Consumes 60 Mana per cast.",
    "condition": {
        "type": "origins:resource",
        "resource": "ucr_stitch:mage/essence_real",
        "comparison": ">=",
        "compare_to": 60
    },
    "type": "origins:active_self",
    "entity_action": {
        "type": "origins:and",
        "actions": [
            {
                "type": "origins:change_resource",
                "resource": "ucr_stitch:mage/essence_real",
                "change": -60,
                "operation": "add"
            },
            {
                "type": "origins:execute_command",
                "command": "tp @s ~ ~0.1 ~"
            },
            {
                "type": "origins:if_else",
                "condition": {
                    "type": "origins:moving"
                },
                "if_action": {
                    "type": "origins:and",
                    "actions": [
                        {
                            "type": "origins:add_velocity",
                            "z": 1.4,
                            "space": "local_horizontal_normalized",
                            "client": true,
                            "server": true,
                            "set": true
                        },
                        {
                            "type": "origins:add_velocity",
                            "y": 0.6,
                            "space": "world",
                            "client": true,
                            "server": true
                        }
                    ]
                },
                "else_action": {
                    "type": "origins:add_velocity",
                    "y": 2,
                    "space": "world",
                    "client": true,
                    "server": true,
                    "set": true
                }
            },
            {
                "type": "origins:set_fall_distance",
                "fall_distance": 0
            },
            {
                "type": "origins:play_sound",
                "sound": "minecraft:entity.horse.jump",
                "volume": 1,
                "pitch": 1.4
            }
        ]
    },
    "cooldown": 1,
    "hud_render": {
        "should_render": false
    },
    "key": {
        "key": "key.origins.primary_active"
    }
}